grades = [100, 100, 90, 40, 80, 100, 85, 70, 90, 65, 90, 85, 50.5]

def print_grades(grades):
    for grade in grades:
        print grade

def grades_sum(grades):
    total = 0
    for grade in grades: 
        total += grade
    return total
    
def grades_average(grades):
    sum_of_grades = grades_sum(grades)
    average = sum_of_grades / float(len(grades))
    return average

def grades_variance(grades):
    average = grades_average(grades)
    variance = 0
    total_variance = 0
    for grade in grades:
        variance += (average - grade) ** 2
    total_variance = variance / len(grades)
    return total_variance
    
print grades_variance(grades)

def grades_std_deviation(total_variance):
    std_dev = 0
    std_dev = total_variance ** 0.5
    return std_dev
    
total_variance = grades_variance(grades)
grades_std_deviation(total_variance)

for grade in grades:
    print grade
print grades_sum(grades)
print grades_average(grades)
print grades_variance(grades)
print grades_std_deviation(total_variance)
